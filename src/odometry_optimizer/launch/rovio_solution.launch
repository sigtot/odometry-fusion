<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <node
            pkg="odometry_optimizer"
            type="odometry_optimizer"
            name="odometry_optimizer"
            required="true"
            output="screen"
    />
    <node
            pkg="odometry_optimizer"
            type="echo_node"
            name="echo_node"
            required="true"
            output="screen"
    />
    <param name="imu_topic_name" value="/imu0"/>
    <param name="odometry1_topic_name" value="/echo/rovio/odometry"/>
    <param name="odometry2_topic_name" value="/echo/aft_mapped_to_init_CORRECTED"/>

    <param name="cov_pos_0" value="0.0001"/>
    <param name="cov_pos_1" value="0.0001"/>
    <param name="cov_pos_2" value="0.0001"/>
    <param name="cov_vel_0" value="1.0"/>
    <param name="cov_vel_1" value="1.0"/>
    <param name="cov_vel_2" value="1.0"/>
    <param name="cov_att_0" value="0.1"/>
    <param name="cov_att_1" value="0.1"/>
    <param name="cov_att_2" value="0.1"/>

    <param name="cov_acb_0" value="0.0004"/>
    <param name="cov_acb_1" value="0.0004"/>
    <param name="cov_acb_2" value="0.0004"/>
    <param name="cov_gyb_0" value="0.0003"/>
    <param name="cov_gyb_1" value="0.0003"/>
    <param name="cov_gyb_2" value="0.0003"/>

    <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
        <param name="filter_config" value="$(find rovio)/cfg/rovio.info"/>
        <param name="camera0_config" value="$(find rovio)/cfg/euroc_cam0.yaml"/>
        <param name="camera1_config" value="$(find rovio)/cfg/euroc_cam1.yaml"/>
    </node>

</launch>
